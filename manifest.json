{
  "manifest_version": 2,
  "name": "Tab time counter",
  "description": "How long have I been on urls?",
  "version": "1.0.0",
  "author": "",
  "permissions": [
    "tabs"
  ],
  "icons": {
    "16": "clock16.png",
    "48": "clock48.png",
    "128": "clock128.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "19": "browser-action-19.png",
      "38": "browser-action-38.png"
    }
  },
  "background": {
    "scripts": [
      "common.js",
      "background.js"
    ],
    "persistent": true
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  }
}